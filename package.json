    "@types/bcryptjs": "^2.4.6",
    "auth": "^1.2.3",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.6.3",
    "imgbb-uploader": "^1.5.1",
    "lucide-react": "^0.503.0",
    "mysql2": "^3.14.0",
    "next": "15.2.2",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "prisma": "^6.5.0",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-google-recaptcha": "^3.1.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-qr-code": "^2.0.15",
    "tailwind-merge": "^3.1.0",
    "uuid": "^11.1.0",
    "zustand": "^5.0.3"
